<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.directstrokenetwork.a20170830.MainActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <Button
            android:id="@+id/send_get_request_btn"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="发送 StringRequest GET请求"
            ></Button>

        <Button
            android:id="@+id/send_post_request_btn"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="发送 StringRequest POST请求"
            ></Button>

        <Button
            android:id="@+id/send_get_gson_request_btn"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="发送 GsonRequest GET请求"
            ></Button>

        <Button
            android:id="@+id/send_post_gson_request_btn"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="发送 GsonRequest POST请求"
            ></Button>

        <Button
            android:id="@+id/gosn_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gson 测试"></Button>

        <TextView
            android:id="@+id/msg_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="返回数据"
            ></TextView>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
